     "text": [
      "0 1 3 4 "
     ]
    }
   ],
   "source": [
    "\n",
    "def selectactivity(s, f):\n",
    "    \n",
    "    x = len(f)\n",
    "    \n",
    "    i = 0\n",
    "    print(i, end=' ')\n",
    "\n",
    "    for j in range(x):\n",
    "        \n",
    "        if s[j] >= f[i]:\n",
    "            print(j, end=' ')\n",
    "            i = j\n",
    "\n",
    "\n",
    "s = [1, 3, 0, 5, 8, 5]\n",
    "f = [2, 4, 6, 7, 9, 9]\n",
    "\n",
    "selectactivity(s, f)\n"
   ]
